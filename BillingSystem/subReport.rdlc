        <CommandText>SELECT        bill_items.id, bill_items.stock_id, bill_items.quantity, bill_items.bill_id, stocks.item_code, stocks.item_name, stocks.item_price
FROM            bill_items INNER JOIN
                         stocks ON bill_items.stock_id = stocks.id</CommandText>
